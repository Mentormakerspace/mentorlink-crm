{
  "version": 2,
  "framework": "nextjs",
  "cleanUrls": true,
  "buildCommand": "pnpm install typescript@5.8.3 @types/react@18.3.20 @types/node@20.17.32 @types/react-dom@18.3.7 --no-frozen-lockfile && pnpm run build",
  "outputDirectory": ".next",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "env": {
    "NODE_ENV": "production",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "buildCommand": "node build.js"
      }
    }
  ]
} 